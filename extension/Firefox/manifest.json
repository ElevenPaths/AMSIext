{
  "description": "Javascript AMSI checker",
  "manifest_version": 2,
  "name": "AMSIext",
  "version": "1.0.2",
  "icons": {
    "16": "icons/normal-16.png",
    "32": "icons/normal-32.png",
    "64": "icons/normal-64.png"
  },

  "applications": {
    "gecko": {
      "id": "amsiext@11paths.com",
      "strict_min_version": "55.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/normal-16.png",
      "32": "icons/normal-32.png",
      "64": "icons/normal-64.png"
    },
    "default_title": "AMSI Extension",
    "default_popup": "popup/index.html"
  },

  "permissions": [
    "nativeMessaging",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "menus",
    "notifications"
  ]
}
